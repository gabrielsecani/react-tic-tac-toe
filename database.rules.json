{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "gameState": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$gameId": {
        ".indexOn": ["createdAt"],
        ".read": "auth != null",
        ".write": "auth != null",
      }
    }
  }
}


