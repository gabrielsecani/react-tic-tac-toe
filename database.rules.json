{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "games": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "gameList": {
      ".indexOn": ["createdAt"],
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
